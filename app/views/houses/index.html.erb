<!DOCTYPE html>
<html lang="en">
    <%= render :partial => :house, :collection => House.all, :as => :c %>
    <div id="wrapper">
        <div class="container">
            <div id="page-content-wrapper">
                <div class="page-header", style: padding-bottom: 100px>
                 <%= link_to((image_tag "/assets/1467816116_More.png", :class => "plus_button_house"), new_house_path) %>
                    <h1>Houses</h1>
                </div>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="row">
                                <div class="col-xs-6 col-md-3">
                                    <ul>
                                        <% @houses.each do |house| %>
                                            <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h3 class="panel-title"> <%= link_to( house.name, house_path(house)) %></h3>
                                            </div>
                                            <div class="panel-body">
                                                <div class="container"  class="zoom-img">
                                                    <div class="row">

                                                        <%= house.user_id  %>
                                                        <%=link_to("Delete", house_path(house), method: :delete) %>
                                                        <%= link_to 'Edit', edit_house_path(house) %>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <% end %>
                                    </ul>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    <!-- /#wrapper -->

