<!DOCTYPE html>
<html lang="en">
<%= render :partial => :house, :collection => House.all, :as => :c %>
 <div id="wrapper">
        <!-- Page Content -->

          <div class="container">
          <div class="page-header", style: padding-bottom: 100px>
          <h1><%= @house.name%></h1>
          </div>

          <%= link_to((image_tag "/assets/1467816116_More.png", :class => "plus_button"),new_house_device_url(@house.id))%>
          <ul>
            <% @house.devices.each do |d| %>
              <div class="panel panel-default" >


                <div class="panel-heading">

                  <h3 class="panel-title"><%=d.name%></h3>
                </div>

                <div class="panel-body">

                  <div class="container"  class="zoom-img">

                  <div class="row">


                  <img src="data:image/svg+xml;base64," alt="..." class="img-rounded" >

                  </div>
              </div>
                </div>
                <div class="panel-footer">
                  <p>
                    </p>
                </div>

            </div>
             <% end %>

          </ul>

          </div>

        <!-- /#page-content-wrapper -->

    </div>
</body>
</html>
